@font-face {
  font-family: 'icon';
  /* project id 647699 */
  src: url("https://at.alicdn.com/t/font_647699_89zbu9adr6f.eot");
  src: url("https://at.alicdn.com/t/font_647699_89zbu9adr6f.eot?#iefix") format("embedded-opentype"), url("https://at.alicdn.com/t/font_647699_89zbu9adr6f.woff2") format("woff2"), url("https://at.alicdn.com/t/font_647699_89zbu9adr6f.woff") format("woff"), url("https://at.alicdn.com/t/font_647699_89zbu9adr6f.ttf") format("truetype"), url("https://at.alicdn.com/t/font_647699_89zbu9adr6f.svg#iconfont") format("svg");
}
.icon {
  display: inline-block;
  font-family: "icon" !important;
  font-size: 24rpx;
  font-style: normal !important;
  font-weight: normal !important;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2rpx;
  -moz-osx-font-smoothing: grayscale;
}
page {
  background: #fff;
  word-break: break-all;
}
text, ._span, view {
  font-size: 24rpx;
  color: #404040;
  text-align: left;
  line-height: 1.5;
}

.vertical-line {
  display: inline-block;
  width: 1px;
  height: 24rpx;
  background: #eaeaea;
}
/* 搜索 */
.icon-search:before {
  content: '\E603';
}
/* 定位 */
.icon-address:before {
  content: '\E650';
}
/* 星星 */
.icon-star-empty:before {
  content: '\E629';
}
.icon-star:before {
  content: '\E628';
}

